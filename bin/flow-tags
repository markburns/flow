#!/usr/bin/env ruby
# frozen_string_literal: true

tags = `git tag`.split("\n").select{|t| t[/^v/]}.map{|s| s.gsub(/^v/i, "")}

semvers = tags.map do |tag|
  tag = tag.gsub(/^v/, '')
  begin
    Gem::Version.new(tag)
  rescue StandardError
    nil
  end
end.compact

puts semvers.sort.map{|s| "v#{s}" }
